@using MVCDesignPattern.Models;


<h3>ViewBag</h3>
<ul>
    @foreach (var product in ViewBag.products)
    {
        <li>@product.ProductName</li>
    }
</ul>


<h3>ViewData</h3>
<ul>
    @foreach (var product in ViewData["products"] as List<Product>)
    {
        <li>@product.ProductName</li>
    }
</ul>

<h3>TempData</h3>
<ul>
    @foreach (var product in TempData["products"] as List<Product>)
    {
        <li>@product.ProductName</li>
    }
</ul>