@addTagHelper * ,Microsoft.AspNetCore.Mvc.TagHelpers

@model List<MVCDesignPattern.Models.Product>

<ul>
    @foreach (var urun in Model)
    {
        <li>
            @urun.ProductName
        </li>
        @ViewBag.Message

    }

</ul>


@TempData["products"]

<form asp-action="Get" asp-controller="Product" method="post"></form>
